{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Wio.Life_Pet.Web.Services": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Connection": "PLACEHOLDER_CONNECTION_STRING"
  },
  "JWTSettings": {
    "SecretKey": "PLACEHOLDER_SECRET_KEY"
  },
  "Docker": {
    "AutoStart": true,
    "MaxStartupWaitMinutes": 5,
    "EnableApiEndpoints": true,
    "StateValidityMinutes": 15
  }
}